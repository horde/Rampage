 <name>Horde_Horde_Rampage</name>
 <channel>pear.horde.org</channel>
 <summary>Horde Rampage Application Server</summary>
 <description>PHP 5 application framework for Horde
 </description>
 <lead>
  <name>Chuck Hagenbuch</name>
  <user>chuck</user>
  <email>chuck@horde.org</email>
  <active>yes</active>
 </lead>
 <date>2007-03-08</date>
 <time>11:00:00</time>
 <version>
  <release>0.1.3</release>
  <api>0.1.3</api>
 </version>
 <stability>
  <release>alpha</release>
  <api>alpha</api>
 </stability>
 <license uri="http://www.gnu.org/copyleft/lesser.html">LGPL</license>
 <notes>Horde_Rampage_Loader is now Horde_Loader
 </notes>
 <contents>
  <dir name="/" baseinstalldir="/Horde">
   <dir name="scripts">
    <dir name="Horde">
     <dir name="Rampage">
      <file name="rampage.php" role="script">
       <tasks:replace from="@php_bin@" to="php_bin" type="pear-config"/>
      </file>
     </dir> <!-- /scripts/Horde/Rampage -->
    </dir> <!-- /scripts/Horde -->
   </dir> <!-- /scripts -->
   <dir name="lib">
    <dir name="Horde">
     <dir name="Rampage">
      <file name="Environment.php" role="php" />
     </dir> <!-- /lib/Horde/Rampage -->
    </dir> <!-- /lib/Horde -->
   </dir> <!-- /lib -->
  </dir> <!-- / -->
 </contents>
 <dependencies>
  <required>
   <php>
    <min>5.2</min>
   </php>
   <pearinstaller>
    <min>1.5</min>
   </pearinstaller>
  </required>
 </dependencies>
 <phprelease>
  <filelist>
   <install name="lib/Horde/Rampage/Environment.php" as="Horde/Rampage/Environment.php" />
   <install name="scripts/Horde/Rampage/rampage.php" as="horde-rampage" />
  </filelist>
 </phprelease>
</package>
